{
  "name": "template-translator",
  "version": "0.0.1",
  "description": "translate templates with composable queries",
  "main": "index.js",
  "scripts": {
    "serve": "set NODE_ENV=development&&webpack-dev-server --hot  --content-base ./ --config ./webpack/development.js",
    "test": "set NODE_ENV=test&&jest",
    "test:watch": "set NODE_ENV=test&&jest --watch",
    "build": "sh ./scripts/build.sh",
    "build:docker": "docker build -t html-translator-frontend .",
    "start": "docker run -p 80:8080 -d html-translator-frontend"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.jsx?$",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "cats.html$": "<rootDir>/test/mocks/cats.js",
      "sample-email.html$": "<rootDir>/test/mocks/sample-email.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/dlizik/dlizik.bitbucket.org.git"
  },
  "author": "daniel lizik",
  "license": "MIT",
  "homepage": "https://bitbucket.org/dlizik/dlizik.bitbucket.org#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "cucumber": "^2.0.0-rc.9",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jest": "^18.1.0",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "file-saver": "^1.3.2",
    "node-sass": "^4.5.2",
    "parse5": "^3.0.1",
    "pug": "^2.0.0-rc.1",
    "pug-cli": "^1.0.0-alpha6",
    "rc-tooltip": "^3.4.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
